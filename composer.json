{
    "name": "cakemanager/cakephp-cakemanager",
    "description": "Plugin for CakePHP 3.x to manage your web-app.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Mulder",
            "email": "bobmulder@outlook.com"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.0.*",
        "cakemanager/cakephp-utils": "dev-master"
    },
    "extra": {
        "installer-name": "CakeManager"
    },
    "autoload": {
        "psr-4": {
            "CakeManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeManager\\Test\\": "tests",
            "CakeManager\\Test\\Fixture\\": "tests/Fixture"
        }
    }
}
